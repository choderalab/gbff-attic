@article{zhao_evaluations_2010,
	title = {Evaluations of the Absolute and Relative Free Energies for Antidepressant Binding to the Amino Acid Membrane Transporter {LeuT} with Free Energy Simulations},
	volume = {6},
	abstract = {The binding of ligands to protein receptors with high affinity and specificity is central to many cellular processes. The quest for the development of computational models capable of accurately evaluating binding affinity remains one of the main goals of modern computational biophysics. In this work, free energy perturbation/molecular dynamics simulations were used to evaluate absolute and relative binding affinity for three different antidepressants to a sodium-dependent membrane transporter, {LeuT,} a bacterial homologue of human serotonin and dopamine transporters. Dysfunction of these membrane transporters in mammals has been implicated in multiple diseases of the nervous system, including bipolar disorder and depression. Furthermore, these proteins are key targets for antidepressants including fluoxetine (aka Prozac) and tricyclic antidepressants known to block transport activity. In addition to being clinically relevant, this system, where multiple crystal structures are readily available, represents an ideal testing ground for methods used to study the molecular mechanisms of ligand binding to membrane proteins. We discuss possible pitfalls and different levels of approximation required to evaluate binding affinity, such as the dependence of the computed affinities on the strength of constraints and the sensitivity of the computed affinities to the particular partial charges derived from restrained electrostatic potential fitting of quantum mechanics electrostatic potential. Finally, we compare the effects of different constraint schemes on the absolute and relative binding affinities obtained from free energy simulations.},
	number = {6},
	journal = {Journal of Chemical Theory and Computation},
	author = {Chunfeng Zhao and David A. Caplan and Sergei Yu. Noskov},
	month = jun,
	year = {2010},
	pages = {1900--1914},
	annote = {{{\textless}p{\textgreater}Closely} follows Roux's methodologies in {CHARMM} to calculate the absolute and free energies of three antidepressants in {LeuT.}?? Unique in that the exact binding affinities are not known, and are hard to predict.?? They explore a range of different simulation parameters such as translation and configuration restraints, and get similar results for relative and absolute binding free energies.{\textless}/p{\textgreater}}
},

@article{zhou_new_2001,
	title = {New linear interaction method for binding affinity calculations using a continuum solvent model},
	volume = {105},
	abstract = {A new linear interaction energy {(LIE)} method based on a continuum solvent surface generalized Born {(SGB)} model is proposed for protein-ligand binding affinity calculations. The new method {SGB-LIE} is about 1 order of magnitude faster than previously published {LIE} methods based on explicit solvents. It has been applied to several binding sets: {HEPT} analogues binding to {HIV-1} reverse transcriptase (20 ligands), sulfonamide inhibitors binding to human thrombin (seven ligands), and various ligands binding to coagulation factor Xa (eight ligands). The {SGB-LIE} predictions and cross-validation results show that about 1.0 kcal/mol accuracy is achievable for binding sets with as many as 20 ligands, e.g., for the {HIV-1} {RT} binding set, {RMS} errors of 1.07 and 1.20 kcal/mol are achieved for {LIE} fitting and leave-one-out cross validation, respectively, with correlation coefficients r(2) equal to 0.774 and 0.717. We have also explored various techniques for the {LIE} underlying conformation space sampling, including molecular dynamics and hybrid Monte Carlo methods, and the final results show that comparable binding energies can be obtained no matter which sampling technique is used.},
	number = {42},
	journal = {Journal of Physical Chemistry B},
	author = {R. H Zhou and Richard A Friesner and A. Ghosh and Robert C Rizzo and William L Jorgensen and Ronald M Levy},
	month = oct,
	year = {2001},
	pages = {10388???10397}
}


@article{Chen:2010p3184,
author = {Wei Chen and Michael K Gilson and Simon P Webb and Michael J Potter}, 
title = {Modeling Protein-Ligand Binding by Mining Minima},
abstract = {We present the first application of the mining minima algorithm to protein???small molecule binding. This end-point approach uses an empirical force field and implicit solvent models, treats the protein binding site as fully flexible, and estimates free energies as sums over local energy wells. The calculations are found to yield encouraging agreement with experimental results for three sets of HIV-1 protease inhibitors and a set of phosphodiesterase 10a inhibitors. The contributions of various aspects of the model to its accuracy are examined, and the Poisson-Boltzmann correction is found to be the most critical. Interestingly, the computed changes in configurational entropy upon binding fall roughly along the same entropy???energy correlation previously observed for smaller host-guest systems. Strengths and weaknesses of the method are discussed, as are the prospects for enhancing accuracy and speed.},
language = {EN},
year = 2010,
date-added = {2010-10-08 14:04:27 -0500},
date-modified = {2010-10-08 14:04:27 -0500},
rating = {0},
journal = {J. Chem. Theory Comput.},
stars = 2,
note = {$\bullet\bullet$ This presents the first application of the Mining Minima approach to protein-ligand binding affinities, an extremely challenging problem. Correlations between computed free energies and experiment are generally good, but some kinks remain to be worked out in terms of absolute agreement with experiment. The Mining Minima approach is particularly intriguing because of its unique approach to sampling, making the fact that it can now be applied to protein-ligand systems even more interesting.}
}


