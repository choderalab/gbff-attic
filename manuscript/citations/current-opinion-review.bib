@article{jorgensen_free-energies_1995,
	title = {{Free-Energies} of Hydration for {Organic-Molecules} from {Monte-Carlo} Simulations},
	volume = {3},
	abstract = {Methods for the computation of free energies of solvation are reviewed, with emphasis on aqueous systems and discrete representation of the solvent molecules in Monte Carlo {(MC)} or molecular dynamics simulations. Precise results for small organic solutes can now be obtained routinely by {MC/free} energy perturbation calculations. The average error for computed free energies of hydration is 1.0 kcal/mol with the use of partial charges for the solutes from either {OPLS} united-atom potential functions or from fitting to the electrostatic potential surface from {6-31G*} ab initio calculations. A less rigorous, computationally more rapid method based on linear response theory is also found to be valuable and reduces the average error for the test set of 16 solutes to 0.6 kcal/mol.},
	journal = {Perspectives in Drug Discovery and Design},
	author = {William L Jorgensen and Julian {Tirado-Rives}},
	year = {1995},
	pages = {123???138}
},

