	pages = {921--944},
	annote = {Accession Number: 13208375; Balzarini, J. 1; Email Address: jan.balzarini@rega.kuleuven.ac.be; Affiliation: 1: Rega Institute for Medical Research, {K.U.Leuven,} B-3000 Leuven, Belgium; Source Info: May2004, Vol. 4 Issue 9, p921; Subject Term: {HIV} {(Viruses);} Subject Term: {RETROVIRUSES;} Subject Term: {REVERSE} transcriptase; Subject Term: {LEAD} compounds; Subject Term: {NUCLEOSIDES;} Subject Term: {CELL} culture; Subject Term: {METABOLITES;} {Author-Supplied} Keyword: delavirdine; {Author-Supplied} Keyword: efavirenz; {Author-Supplied} Keyword: nevirapine; {Author-Supplied} Keyword: non-nucleoside; {Author-Supplied} Keyword: nucleoside rt inhibitors; {Author-Supplied} Keyword: reverse transcriptase inhibitors; Number of Pages: 24p; Document Type: Article},
